  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "test": "mocha --harmony --compilers js:babel/register 'test/*/*.js' 'playground/todo/test/*.js'",
    "test:watch": "npm test -- --watch",
    "test:ci": "npm test -- --reporter mocha-teamcity-reporter",
    "todo:cli": "node --harmony -e \"require('babel/register'); require('./playground/todo/cli')\"",
    "todo:http-api": "node --harmony -e \"require('babel/register'); require('./playground/todo/http-api')\""
  },
  "devDependencies": {
    "babel": "^5.8.3",
    "mocha": "^2.2.5",
    "mocha-teamcity-reporter": ">=0.0.1",
    "mock-fs": "https://github.com/city41/mock-fs.git",
    "tmp": "0.0.26"
  },
  "dependencies": {
    "bluebird": "^2.9.34",
    "hapi": "^8.8.1",
    "immutable": "^3.7.4",
    "joi": "^6.6.1",
    "lodash": "^3.10.0",
    "lout": "^6.2.3",
    "node-uuid": "^1.4.3",
    "object-hash": "^0.8.0"
  }
